{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "new-cap": [2, { "capIsNewExceptions": ["List", "Map"] }],
        "max-len": [2, {"code": 120, "ignoreUrls": true}],
    }
}
